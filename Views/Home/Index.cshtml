@{
    ViewData["Title"] = "Nefi's Top 5 Restaurants";
}

@model IEnumerable<string>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width-device-width" />
</head>

<body>
    <header>
        <h4 class="bg-primary text-white text-center">Nefi's Top 5 Restaurants</h4>
    </header>
    <div class="list-group">
        <ul>

            @foreach (string i in Model)
            {
            <li class="list-group-item">@Html.Raw(i)</li>
            }
        </ul>
    </div>
</body>
</html>
